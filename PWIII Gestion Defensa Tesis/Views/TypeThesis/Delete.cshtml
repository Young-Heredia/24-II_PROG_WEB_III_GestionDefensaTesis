@model PWIII_Gestion_Defensa_Tesis.Models.TypeThesis

<h1>Delete Thesis Type</h1>

<h4>Are you sure you want to delete this thesis type?</h4>
<div>
    <dl class="row">
        <dt>Name</dt>
        <dd>@Model.Name</dd>
        <dt>Registration Date</dt>
        <dd>@Model.registerDate.ToString("dd/MM/yyyy")</dd>
    </dl>
</div>

<form asp-action="Delete">
    <input type="hidden" asp-for="Id" />
    <button type="submit" class="btn btn-danger">Delete</button>
    <a asp-action="Index" class="btn btn-secondary">Cancel</a>
</form>
